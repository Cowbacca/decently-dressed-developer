{
  "name": "decently-dressed-developer",
  "description": "Decently Dressed Developer generator powered by Gatsby",
  "version": "1.0.0",
  "author": "Angus J Goldmsith <angus@dashery.co.uk>",
  "dependencies": {
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-helmet": "^5.0.3",
    "dotenv": "^4.0.0",
    "gatsby": "^1.9.145",
    "gatsby-link": "^1.6.32",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-typescript": "^1.4.12",
    "gatsby-source-contentful": "^1.3.31"
  },
  "keywords": [
    "gatsby",
    "decently",
    "dressed",
    "developer"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "typecheck": "tsc --noemit",
    "lint": "tslint 'src/*.ts?(x)' 'src/**/*.ts?(x)'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "gatsby build && gatsby serve",
    "graphql-schema": "apollo-codegen introspect-schema http://localhost:8000/___graphql --output schema.json",
    "graphql-queries": "apollo-codegen generate **/*.tsx --target typescript --tag-name graphql --schema schema.json --output src/graghql-query-types.ts"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.3",
    "apollo-codegen": "^0.18.3",
    "graphql-code-generator": "^0.8.14",
    "prettier": "^1.9.2",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2"
  }
}
